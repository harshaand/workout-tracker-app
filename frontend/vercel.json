{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/static/js/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        },
        {
            "source": "/static/css/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        }
    ]
}